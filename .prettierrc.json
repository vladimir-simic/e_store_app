{
  "useTabs": false,
  "printWidth": 100,
  "tabWidth": 2,
  "singleQuote": true,
  "trailingComma": "all",
  "jsxBracketSameLine": true,
  "parser": "babylon",
  "semi": true,
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "overrides": [
    {
        "files": ".json",
        "options": {
            "parser": "json"
        }
    }
  ]
}
